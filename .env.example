# Copy this file to .env and fill in your values, then run commands like:
# docker compose run --rm aws-finops --all
# docker compose run --rm aws-finops --profiles dev prod --regions us-east-1
# docker compose run --rm aws-finops --audit

# Optional: pick a default region
# AWS_DEFAULT_REGION=us-east-1

# Optional: pick a specific profile from mounted ~/.aws files
# AWS_PROFILE=default

# Optional: Slack reporting
# SLACK_BOT_TOKEN=xoxb-your-token
